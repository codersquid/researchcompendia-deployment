function ff() { find . -name '*'$1'*' ; }

if [ -f /usr/local/bin/virtualenvwrapper.sh ]; then
    . /usr/local/bin/virtualenvwrapper.sh
    export WORKON_HOME=${HOME}/venvs
fi

# colorizes and names prompt according to git repo and branch
# git@github.com:codersquid/bash-help.git
if [ -f ~/.bash-help/git-simple-prompt.sh ]; then
    . ~/.bash-help/git-simple-prompt.sh
    PROMPT_COMMAND=git_simple_prompt
fi
